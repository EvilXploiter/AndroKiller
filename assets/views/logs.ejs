<!DOCTYPE html>
<html>

<% include partials/head.ejs %>
<head>
<style>
    body {
  --s: 50px;
  --c: #191b22;
  --_s: calc(2*var(--s)) calc(2*var(--s));
  --_g: 35.36% 35.36% at;
  --_c: #0000 66%,#20222a 68% 70%,#0000 72%;
  background: 
    radial-gradient(var(--_g) 100% 25%,var(--_c)) var(--s) var(--s)/var(--_s), 
    radial-gradient(var(--_g) 0 75%,var(--_c)) var(--s) var(--s)/var(--_s), 
    radial-gradient(var(--_g) 100% 25%,var(--_c)) 0 0/var(--_s), 
    radial-gradient(var(--_g) 0 75%,var(--_c)) 0 0/var(--_s), 
    repeating-conic-gradient(var(--c) 0 25%,#0000 0 50%) 0 0/var(--_s), 
    radial-gradient(var(--_c)) 0 calc(var(--s)/2)/var(--s) var(--s) var(--c);
  background-attachment: fixed;
}
</style>
</head>
<body>
    <script>
        gtag('event', 'screen_view', {
            'screen_name': 'logs',
            'app_name': 'AndroKiller'
        });
    </script>
    <div class="ui container">
        <% include partials/header.ejs %>
        <div class="ui segment" style="background-image: url('https://i.ibb.co/zPsMH9D/background.jpg'); ">
            <div class="logs">
                <% logs.forEach((log) => { %>
                <div class="logitm <%= log.type %>">
                    <%= new Date(log.time).toLocaleString('en-GB', { timeZone: 'UTC' }) %> =>
                    <%= log.type %> | <%= log.message %> </div>
                <% }) %>
            </div>
        </div>
    </div>
    <% include partials/footer.ejs %>
</body>

</html>